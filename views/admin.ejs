<div style="position: fixed; top: 80px; left: 0; background: #F5D061; color: #2A363B; width: 100%; height: 3em;">
  <ul class="nav menuItems" id="myTab" style="justify-content: center;">
    <li class="nav-item menuItems">
      <a class="nav-item text-dark" href="#about">About</a>
    </li>
    <li class="nav-item menuItems">
      <a class="nav-item text-dark" href="#ranking">Ranking</a>
    </li>
    <li class="nav-item menuItems">
      <a class="nav-item text-dark" href="#results">Results</a>
    </li>
    <li class="nav-item menuItems">
      <a class="nav-item text-dark" href="#tournaments">Opens</a>
    </li>
  </ul>
</div>
<div>
  <div class="tab-content" id="v-pills-tabContent">
    <div class="tab-pane fade" id="about" role="tabpanel">
        <%- include('./admin_partials/admin_about.ejs', {content: content}) %>	
    </div>
    <div class="tab-pane fade" id="ranking" role="tabpanel">
        <%- include('./admin_partials/admin_ranking.ejs', {players: players}) %>	
    </div>
    <div class="tab-pane fade" id="results" role="tabpanel">
        <%- include('./admin_partials/admin_results.ejs', {results: results, players: players, opens: opens}) %>	
    </div>
    <div class="tab-pane fade" id="opens" role="tabpanel">
        <%- include('./admin_partials/admin_opens.ejs', {opens: opens}) %>	
    </div>
  </div>
</div>
  
  <script>
    $('#myTab a').on('click', function (e) {
      e.preventDefault()
      $(this).tab('show')
    })
  </script>
