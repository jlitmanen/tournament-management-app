<div class="container">
    <table class="table table-bordered table-hover">
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>Group</th>
            <th>Points</th>
        </tr>
        <% players?.forEach(function(c, i) { %>
            <tbody>
                <tr>
                    <td> <%= i + 1 %>.      </td>
                    <td> <%= c.name %>      </td>
                    <td> <%= c.group %>     </td>
                    <td> <%= c.points %>    </td>
                </tr>
            </tbody>
        <% }) %>
    </table>
</div>